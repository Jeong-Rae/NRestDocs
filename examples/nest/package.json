{
    "name": "example-nest",
    "version": "0.0.1",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "pnpm exec biome format --write",
        "start": "nest start -b swc",
        "start:dev": "nest start -w -b swc",
        "start:debug": "nest start --debug -w -b swc",
        "start:prod": "node dist/main",
        "lint": "pnpm exec biome check --write",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:cov": "vitest run --coverage",
        "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
        "test:e2e": "vitest run --config ./vitest.config.e2e.ts"
    },
    "dependencies": {
        "@nestjs/common": "^11.0.1",
        "@nestjs/core": "^11.0.1",
        "@nestjs/platform-express": "^11.0.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.1",
        "@nrestdocs/core": "workspace:*",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.11.22",
        "@types/express": "^5.0.0",
        "@types/node": "^22.15.2",
        "@types/supertest": "^6.0.3",
        "@vitest/coverage-v8": "3.1.1",
        "globals": "^16.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.1.0",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3",
        "unplugin-swc": "^1.5.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.1"
    }
}
